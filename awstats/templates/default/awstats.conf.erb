# AWStats - generated by CHEF, do NOT change

<% @node[:awstats][:aliases][:static].each do |al| %>
Alias <%= al[0] %> "<%= al[1] %>"
<% end %>

<% @node[:awstats][:aliases][:script].each do |al| %>
ScriptAlias <%= al[0] %> "<%= al[1] %>"
<% end %>

<Directory "<%= @node[:awstats][:config][:dir_data]%>">
  Options None
  AllowOverride None
  Order deny,allow
  Deny from all
<% if @node[:awstats][:allow_from].is_a? Array %>
  <% @node[:awstats][:allow_from].each do |allow| %>
  Allow from <%= allow %>
  <% end %>
<% end %>
</Directory>
